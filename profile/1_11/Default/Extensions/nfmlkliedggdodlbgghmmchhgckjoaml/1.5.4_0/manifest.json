{
   "action": {
      "default_icon": {
         "128": "images/128.png",
         "256": "images/256.png",
         "32": "images/32.png",
         "48": "images/48.png",
         "64": "images/64.png"
      },
      "default_title": "Blockify: Spotify Ad Blocker [Alt + S]"
   },
   "author": "Spotify Ad Blocker | Blockify",
   "background": {
      "service_worker": "background.js"
   },
   "commands": {
      "open_spotify": {
         "description": "Press Alt + S to open Spotify",
         "suggested_key": {
            "chromeos": "Alt+S",
            "default": "Alt+S",
            "linux": "Alt+S",
            "mac": "Alt+S",
            "windows": "Alt+S"
         }
      }
   },
   "content_scripts": [ {
      "css": [ "styles.css" ],
      "js": [ "content_script.js", "mute.js" ],
      "matches": [ "https://open.spotify.com/*" ],
      "run_at": "document_start"
   }, {
      "js": [ "adi.js" ],
      "matches": [ "https://open.spotify.com/*" ],
      "run_at": "document_idle"
   } ],
   "declarative_net_request": {
      "rule_resources": [ {
         "enabled": true,
         "id": "ruleset_1",
         "path": "rules.json"
      } ]
   },
   "description": "Blockify is a Spotify Ad Blocker that blocks & removes audio ads from Spotify using Spotify adblock technology.",
   "homepage_url": "https://getblockify.com",
   "host_permissions": [ "*://open.spotify.com/*" ],
   "icons": {
      "128": "images/128.png",
      "256": "images/256.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "64": "images/64.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Spotify Ad Blocker - Blockify",
   "offline_enabled": true,
   "optional_host_permissions": [ "*://*.spotify.com/*" ],
   "permissions": [ "declarativeNetRequest", "storage", "background" ],
   "short_name": "Spotify Ad Blocker",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.5.4",
   "web_accessible_resources": [ {
      "all_frames": true,
      "extension_ids": [ "*" ],
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "*" ]
   } ]
}
